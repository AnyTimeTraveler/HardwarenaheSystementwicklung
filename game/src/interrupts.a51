NAME interrupts

EXTRN CODE (PJMP_INIT, PJMPI_REFRESH_SCREEN, PJMPI_SUB_GAMETICK, PJMPI_KEYBOARD_ISR)

; ISR_RESET
CSEG AT 0x0000
    JMP PJMP_INIT

; ISR_EXTERN_0
CSEG AT 0x0003
    RETI

; ISR_TIMER_0
CSEG AT 0x000B
    JMP PJMPI_REFRESH_SCREEN

; ISR_EXTERN_1
CSEG AT 0x0013
    RETI

; ISR_TIMER_1
CSEG AT 0x001B
    JMP PJMPI_SUB_GAMETICK

; ISR_SERIAL
CSEG AT 0x0023
    JMP PJMPI_KEYBOARD_ISR

; ISR_TIMER_2
CSEG AT 0x002B
    RETI

END
